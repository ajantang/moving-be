POST {{baseUrl}}/quotes
Content-Type: application/json
Cookie: accessToken={eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpZCI6MSwiY3VzdG9tZXJJZCI6MSwibW92ZXJJZCI6bnVsbCwiaWF0IjoxNzMzNTczNDQ0LCJleHAiOjE3MzM1NzcwNDR9.iIHpqGeDCoeLdu-Bn1N7OfKV3PXoAckSvw_B0Yk4eCk}

